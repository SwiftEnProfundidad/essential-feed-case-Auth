{
    "sweetpad.build.xcodeWorkspacePath": "EssentialApp/EssentialApp.xcworkspace",
    "window.autoDetectColorScheme": true,
    "window.confirmSaveUntitledWorkspace": true,
    "interactiveWindow.promptToSaveOnClose": true,
    "markdown.validate.enabled": true,
    "files.autoSave": "onFocusChange",
    "testExplorer.addToEditorContextMenu": true,
    "diffEditor.experimental.showEmptyDecorations": false,
    "markdown.suggest.paths.includeWorkspaceHeaderCompletions": "never",
    "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
    "javascript.suggest.autoImports": false,
    "javascript.updateImportsOnPaste.enabled": false,
    "typescript.preferences.includePackageJsonAutoImports": "off",
    "typescript.suggest.autoImports": false,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "typescript.updateImportsOnPaste.enabled": false,
    "css.lint.argumentsInColorFunction": "ignore",
    "css.lint.emptyRules": "ignore",
    "css.lint.fontFaceProperties": "ignore",
    "css.lint.propertyIgnoredDueToDisplay": "ignore",
    "css.lint.unknownAtRules": "ignore",
    "css.lint.unknownProperties": "ignore",
    "css.lint.vendorPrefix": "ignore",
    "less.lint.argumentsInColorFunction": "ignore",
    "less.lint.emptyRules": "ignore",
    "less.lint.fontFaceProperties": "ignore",
    "less.lint.propertyIgnoredDueToDisplay": "ignore",
    "less.lint.unknownAtRules": "ignore",
    "less.lint.unknownProperties": "ignore",
    "less.lint.vendorPrefix": "ignore",
    "scss.lint.emptyRules": "ignore",
    "scss.lint.fontFaceProperties": "ignore",
    "scss.lint.propertyIgnoredDueToDisplay": "ignore",
    "scss.lint.unknownAtRules": "ignore",
    "scss.lint.unknownProperties": "ignore",
    "scss.lint.vendorPrefix": "ignore"
}